<?xml version="1.0" encoding="utf-8"?>
<!-- minimal manifest for webEdition applications -->
<!-- values not found here are obtained from /webEdition/lib/we/app/defaults/manifest.xml -->
<!-- a sample showing all currently supported values can be found at /webEdition/lib/we/app/defaults/sample-manifest.xml -->
<manifest>
	
	<creator>
		<company>webEdition Project</company>
		<name>webEdition e.V.</name>
		<address>Adresse</address>
        <country>DE</country>
		<phone></phone>
		<fax></fax>
		<email>applications@webedition.org</email>
		<www>http://www.webedition.org</www>
		<year>2010</year>
	</creator>
	
	<info>
		<implementation>tool</implementation>
		<name>{$TOOLNAMELANG}</name>
		<classname>{$TOOLNAME}</classname>
		<!-- name of the app, at least de and en should be supplied -->
		<title>
			<de>App Name</de>
			<en>app name</en>
		</title>
		<!-- a small description of the app, at least de and en should be supplied -->
		<description>
			<de>Beschreibung</de>
			<en>description</en>
		</description>
		<version>0.01</version>
		<license>GPL3</license>
		<licenseurl>http://www.gnu.org/licenses/gpl.html</licenseurl>
		<encoding>none</encoding>
		<supportshooks>true</supportshooks>
		<usewetableprefix>true</usewetableprefix>
		<deactivatable>true</deactivatable>
		<deinstallable>true</deinstallable>
		<updatable>true</updatable>
		<categories>
			<category>miscellaneous</category>
		</categories>
	</info>
	
	<requirements>
      <php-version>5.2.4</php-version>
      <php-ext>simplexml</php-ext>
      <php-ext>gdlib</php-ext>
	</requirements>
	
	<dependencies>
		<version>{$WEVERSION}</version>
		<sdkversion>{$SDKVERSION}</sdkversion>
	</dependencies>
	
</manifest>